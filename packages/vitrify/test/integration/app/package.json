    "@vue/shared": "3.5.29",
    "beasties": "0.4.1",
    "devcert": "1.2.3",
    "eslint": "10.0.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-vue": "10.8.0",
    "fastify": "5.7.4",
    "npm-run-all": "4.1.5",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1",
    "vite": "7.3.1",
    "vite-plugin-pwa": "1.2.0",
    "vitrify": "0.25.8",
