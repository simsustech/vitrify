    "ajv": "^8.17.1",
    "animated-unocss": "^0.0.6",
    "cac": "^6.7.14",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.3",
    "fastify": "^5.3.2",
