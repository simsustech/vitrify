{
  "name": "vitrify",
  "version": "0.11.9",
  "license": "MIT",
  "author": "Stefan van Herwijnen",
  "description": "Vite as your Full Stack development tool",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "vitrify": "./dist/bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./build": {
      "types": "./dist/types/bin/build.d.ts",
      "import": "./dist/bin/build.js"
    },
    "./dev": {
      "types": "./dist/types/bin/dev.d.ts",
      "import": "./dist/bin/dev.js"
    },
    "./prerender": {
      "types": "./dist/types/bin/prerender.d.ts",
      "import": "./dist/bin/prerender.js"
    },
    "./helpers/*": "./dist/helpers/*.js",
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simsustech/vitrify.git",
    "directory": "packages/vitrify"
  },
  "bugs": {
    "url": "https://github.com/simsustech/vitrify/issues"
  },
  "homepage": "https://github.com/simsustech/vitrify/tree/main/#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest test/"
  },
  "dependencies": {
    "@fastify/middie": "^8.1.0",
    "@fastify/static": "^6.9.0",
    "@quasar/extras": "^1.15.11",
    "@vitejs/plugin-vue": "^4.0.0",
    "ajv": "^8.12.0",
    "builtin-modules": "^3.3.0",
    "cac": "^6.7.14",
    "chalk": "^5.2.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.8",
    "fastify": "^4.13.0",
    "glob": "^8.1.0",
    "happy-dom": "^8.4.0",
    "is-port-reachable": "^4.0.0",
    "magic-string": "^0.29.0",
    "merge-deep": "^3.0.3",
    "readline": "^1.3.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "1.58.1",
    "ts-node": "^10.9.1",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vitest": "^0.28.5"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@types/glob": "^8.0.1",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^18.13.0",
    "@types/ws": "^8.5.4",
    "@vue/runtime-core": "^3.2.47",
    "quasar": "^2.11.6",
    "rollup": "^3.15.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "peerDependencies": {
    "@fastify/static": "^6.5.0",
    "fastify": "^4.5.3",
    "quasar": "^2.7.7",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.js"
  ],
  "keywords": [
    "vite",
    "full stack",
    "fastify"
  ]
}
