{
  "name": "vitrify",
  "version": "0.16.2",
  "license": "MIT",
  "author": "Stefan van Herwijnen",
  "description": "Vite as your Full Stack development tool",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "vitrify": "./dist/bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./build": {
      "types": "./dist/types/bin/build.d.ts",
      "import": "./dist/bin/build.js"
    },
    "./dev": {
      "types": "./dist/types/bin/dev.d.ts",
      "import": "./dist/bin/dev.js"
    },
    "./prerender": {
      "types": "./dist/types/bin/prerender.d.ts",
      "import": "./dist/bin/prerender.js"
    },
    "./helpers/*": "./dist/helpers/*.js",
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simsustech/vitrify.git",
    "directory": "packages/vitrify"
  },
  "bugs": {
    "url": "https://github.com/simsustech/vitrify/issues"
  },
  "homepage": "https://github.com/simsustech/vitrify/tree/main/#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest test/"
  },
  "dependencies": {
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "ajv": "^8.17.1",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "critters": "^0.0.25",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.0",
    "fastify": "^5.1.0",
    "glob": "^11.0.0",
    "happy-dom": "^15.11.7",
    "is-port-reachable": "^4.0.0",
    "magic-string": "^0.30.14",
    "merge-deep": "^3.0.3",
    "readline": "^1.3.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "1.82.0",
    "ts-node": "^10.9.2",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.3",
    "vite-plugin-pwa": "^0.21.1",
    "vitefu": "^1.0.4",
    "vitest": "^2.1.8",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@quasar/extras": "^1.16.15",
    "@types/connect": "^3.4.38",
    "@types/glob": "^8.1.0",
    "@types/merge-deep": "^3.0.3",
    "@types/node": "^22.10.1",
    "@types/ws": "^8.5.13",
    "@vue/runtime-core": "^3.5.13",
    "quasar": "^2.17.4",
    "rollup": "^4.28.1",
    "typescript": "^5.7.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "peerDependencies": {
    "@fastify/static": "^8.0.3",
    "fastify": "^5.1.0",
    "quasar": "^2.17.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.js"
  ],
  "keywords": [
    "vite",
    "full stack",
    "fastify"
  ]
}
