{
  "name": "vitrify",
  "version": "0.5.3",
  "license": "MIT",
  "author": "Stefan van Herwijnen",
  "description": "Pre-configured Vite CLI for your framework",
  "type": "module",
  "bin": {
    "vitrify": "./dist/bin/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./build": "./dist/bin/build.js",
    "./dev": "./dist/bin/dev.js",
    "./helpers/*": "./dist/helpers/*.js",
    "./vite/*": "./src/vite/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ],
      "helpers/*": [
        "./dist/types/helpers/*.d.ts"
      ],
      "build": [
        "./dist/types/bin/build.d.ts"
      ],
      "dev": [
        "./dist/types/bin/dev.d.ts"
      ],
      "help": [
        "./dist/types/bin/help.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simsustech/vitrify.git",
    "directory": "packages/vitrify"
  },
  "bugs": {
    "url": "https://github.com/simsustech/vitrify/issues"
  },
  "homepage": "https://github.com/simsustech/vitrify/tree/main/#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest test/"
  },
  "dependencies": {
    "@fastify/middie": "^8.0.0",
    "@fastify/static": "^6.4.0",
    "@quasar/extras": "^1.14.0",
    "@vitejs/plugin-vue": "^3.0.0-alpha.1",
    "builtin-modules": "^3.3.0",
    "cac": "^6.7.12",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.43",
    "fastify": "^4.0.0",
    "glob": "^8.0.3",
    "happy-dom": "^5.2.0",
    "import-meta-resolve": "^2.0.3",
    "local-pkg": "^0.4.1",
    "magic-string": "^0.26.2",
    "merge-deep": "^3.0.3",
    "readline": "^1.3.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "1.52.3",
    "ts-node": "^10.8.1",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^3.0.0-beta.0",
    "vitest": "^0.14.1"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@types/glob": "^7.2.0",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^17.0.41",
    "@types/ws": "^8.5.3",
    "@vue/runtime-core": "^3.2.37",
    "quasar": "^2.7.1",
    "rollup": "^2.75.6",
    "typescript": "^4.7.3",
    "vite": "^3.0.0-alpha.11",
    "vue": "^3.2.37",
    "vue-router": "^4.0.15"
  },
  "peerDependencies": {
    "@fastify/static": "^6.4.0",
    "fastify": "^4.0.0",
    "import-meta-resolve": "^2.0.3",
    "quasar": "^2.7.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.15"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.js",
    "!dist/**/test.js"
  ]
}
