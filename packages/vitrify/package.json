{
  "name": "vitrify",
  "version": "0.2.3",
  "license": "MIT",
  "author": "Stefan van Herwijnen",
  "description": "Pre-configured Vite CLI for your framework",
  "type": "module",
  "bin": {
    "vitrify": "./dist/bin/cli.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./build": "./dist/bin/build.js",
    "./dev": "./dist/bin/dev.js",
    "./helpers/*": "./dist/helpers/*.js",
    "./vite/*": "./src/vite/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ],
      "helpers/*": [
        "./dist/types/helpers/*.d.ts"
      ],
      "build": [
        "./dist/types/bin/build.d.ts"
      ],
      "dev": [
        "./dist/types/bin/dev.d.ts"
      ],
      "help": [
        "./dist/types/bin/help.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simsustech/vitrify.git",
    "directory": "packages/vitrify"
  },
  "bugs": {
    "url": "https://github.com/simsustech/vitrify/issues"
  },
  "homepage": "https://github.com/simsustech/vitrify/tree/main/#readme",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.5",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/server-renderer": "^3.2.33",
    "builtin-modules": "^3.2.0",
    "cac": "^6.7.12",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "fastify": "^3.28.0",
    "fastify-static": "^4.6.1",
    "glob": "^8.0.1",
    "happy-dom": "^2.55.0",
    "import-meta-resolve": "^1.1.1",
    "local-pkg": "^0.4.1",
    "magic-string": "^0.26.1",
    "merge-deep": "^3.0.3",
    "middie": "^6.0.0",
    "readline": "^1.3.0",
    "sass": "1.50.0",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^17.0.24",
    "@types/ws": "^8.5.3",
    "@vue/runtime-core": "^3.2.33",
    "quasar": "^2.6.6",
    "rollup": "^2.70.1",
    "typescript": "^4.6.3",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "peerDependencies": {
    "fastify": "^3.28.0",
    "fastify-plugin": "^3.0.1",
    "fastify-sensible": "^3.1.2",
    "fastify-static": "^4.6.1",
    "quasar": "^2.6.6",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.js",
    "!dist/**/test.js"
  ]
}
