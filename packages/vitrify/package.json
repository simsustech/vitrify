{
  "name": "vitrify",
  "version": "0.15.2",
  "license": "MIT",
  "author": "Stefan van Herwijnen",
  "description": "Vite as your Full Stack development tool",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "vitrify": "./dist/bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./build": {
      "types": "./dist/types/bin/build.d.ts",
      "import": "./dist/bin/build.js"
    },
    "./dev": {
      "types": "./dist/types/bin/dev.d.ts",
      "import": "./dist/bin/dev.js"
    },
    "./prerender": {
      "types": "./dist/types/bin/prerender.d.ts",
      "import": "./dist/bin/prerender.js"
    },
    "./helpers/*": "./dist/helpers/*.js",
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simsustech/vitrify.git",
    "directory": "packages/vitrify"
  },
  "bugs": {
    "url": "https://github.com/simsustech/vitrify/issues"
  },
  "homepage": "https://github.com/simsustech/vitrify/tree/main/#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest test/"
  },
  "dependencies": {
    "@fastify/middie": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "ajv": "^8.16.0",
    "builtin-modules": "^4.0.0",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "critters": "^0.0.22",
    "cross-env": "^7.0.3",
    "esbuild": "^0.21.4",
    "fastify": "^4.27.0",
    "glob": "^10.4.1",
    "happy-dom": "^14.12.0",
    "is-port-reachable": "^4.0.0",
    "magic-string": "^0.30.10",
    "merge-deep": "^3.0.3",
    "readline": "^1.3.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "1.77.4",
    "ts-node": "^10.9.2",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.12",
    "vite-plugin-pwa": "^0.20.0",
    "vitefu": "^0.2.5",
    "vitest": "^1.6.0",
    "workbox-window": "^7.1.0"
  },
  "devDependencies": {
    "@quasar/extras": "^1.16.11",
    "@types/connect": "^3.4.38",
    "@types/glob": "^8.1.0",
    "@types/merge-deep": "^3.0.3",
    "@types/node": "^20.14.2",
    "@types/ws": "^8.5.10",
    "@vue/runtime-core": "^3.4.27",
    "quasar": "^2.16.4",
    "rollup": "^4.18.0",
    "typescript": "^5.4.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "peerDependencies": {
    "@fastify/static": "^7.0.1",
    "fastify": "^4.26.0",
    "quasar": "^2.14.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.js"
  ],
  "keywords": [
    "vite",
    "full stack",
    "fastify"
  ]
}
